<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Widget.BottomNavigationView" parent="Widget.Design.BottomNavigationView">
        <item name="fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/textSize_12sp</item>
        <item name="itemIconTint">@drawable/selector_navigation</item>
        <item name="itemTextColor">@drawable/selector_navigation</item>
        <item name="backgroundTint">@color/white</item>
    </style>

    <style name="developers">
        <item name="android:layout_marginStart">@dimen/margin_16dp</item>
        <item name="android:layout_marginEnd">@dimen/margin_16dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="title">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/margin_16dp</item>
        <item name="android:layout_marginTop">@dimen/margin_19dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">?attr/blackWhiteColor</item>
        <item name="android:textSize">@dimen/textSize_22sp</item>
    </style>

    <style name="textMedium22">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/textSize_22sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="textRegular16">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="textRegular12">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/textSize_12sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="hintTextColor">?attr/colorOnSecondary</item>
        <item name="android:drawable">@drawable/bg_et_search</item>
        <item name="endIconDrawable">@drawable/search</item>
        <item name="boxStrokeColor">@color/box_stroke</item>
    </style>

    <style name="ThemeOverlay.AppTheme.TextInputEditText.Outlined" parent="">
        <item name="colorControlActivated">@color/blue</item>
    </style>

    <style name="imageBtnJobStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="job16TextMedium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:fontWeight">@dimen/fontWeight_500</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="job16TextRegular">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:fontWeight">@dimen/fontWeight_400</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="job22Text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/textSize_22sp</item>
        <item name="android:fontWeight">@dimen/fontWeight_500</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="job32Text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_bold</item>
        <item name="android:textSize">@dimen/textSize_32sp</item>
        <item name="android:fontWeight">@dimen/fontWeight_700</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="jobCard16Text">
        <item name="android:layout_width">@dimen/jobCardTextLength</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:fontWeight">@dimen/fontWeight_400</item>
        <item name="android:textColor">@color/black_universal</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="jobCard22Text">
        <item name="android:layout_width">@dimen/jobCardTextLength</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/textSize_22sp</item>
        <item name="android:fontWeight">@dimen/fontWeight_500</item>
        <item name="android:textColor">@color/black_universal</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="jobImageStyle">
        <item name="android:layout_width">@dimen/iconSize_48dp</item>
        <item name="android:layout_height">@dimen/iconSize_48dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="ButtonLong">
        <item name="cornerRadius">@dimen/radius_12dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/white_universal</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="MenuPointsStyle">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="fontFamily">@font/ys_display_regular</item>
        <item name="fontWeight">500</item>
        <item name="drawableTint">?attr/colorOnPrimary</item>
    </style>

    <style name="textHint12">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/textSize_12sp</item>
        <item name="android:textColorHint">?attr/colorOnSecondary</item>
    </style>

    <style name="textHint16">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
    </style>

    <style name="ButtonAccept">
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="backgroundTint">@color/blue</item>
        <item name="cornerRadius">@dimen/radius_12dp</item>
        <item name="android:textColor">@color/white_universal</item>
    </style>

    <style name="ButtonReset">
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="backgroundTint">@color/white</item>
        <item name="cornerRadius">@dimen/radius_12dp</item>
        <item name="android:textColor">@color/red</item>
    </style>

    <style name="FilterItemTextRegular16">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/textSize_16sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:lines">1</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="SalaryTextInputStyle" parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="hintTextColor">@color/blue</item>
        <item name="android:background">@drawable/bg_et_search</item>
        <item name="android:textColorHint">?attr/colorOnSecondary</item>
        <item name="boxStrokeWidth">0dp</item>
        <item name="boxStrokeWidthFocused">0dp</item>
        <item name="endIconDrawable">@drawable/cross</item>
        <item name="endIconMode">clear_text</item>
        <item name="endIconTint">@color/black_universal</item>
        <item name="expandedHintEnabled">false</item>
        <item name="endIconScaleType">center</item>
        <item name="hintTextAppearance">@style/textHint12</item>
        <item name="colorControlActivated">@color/blue</item>
    </style>

    <style name="SalaryTheme" parent="">
        <item name="colorControlActivated">@color/blue</item>
    </style>

    <style name="SalaryEt" parent="@style/textHint16">
        <item name="android:textColorHint">?attr/colorOnSecondary</item>
        <item name="android:inputType">number</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxLength">9</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/black_universal</item>
    </style>
</resources>